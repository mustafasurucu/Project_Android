<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.intentdemo.MainActivity" >

    <Button
        android:id="@+id/start_browser"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/start_browser" />

    <Button
        android:id="@+id/start_browser_b"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/start_browser_b" />

    <Button
        android:id="@+id/start_browser_c"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/start_browser_c" />

    <Button
        android:id="@+id/start_phone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/start_phone" />

</LinearLayout>